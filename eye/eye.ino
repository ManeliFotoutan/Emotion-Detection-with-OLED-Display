void setup() {
  Serial.begin(9600);
  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;);
  }
  display.clearDisplay();
  display.display();
}

void loop() {
  // Check if data is available to read
  while (Serial.available() > 0) {
    incomingEmotion = Serial.readStringUntil('\n');
    incomingEmotion.trim(); 
    
    if (incomingEmotion == "happy") {
      display.clearDisplay();  // Clear the buffer
      display.drawBitmap(0, 0, image_data_SECONDarray, 128, 64, 1);
      display.display();    // Example action for "happy" emotion
    } else if (incomingEmotion == "sad") {
      display.clearDisplay();  // Clear the buffer
      display.drawBitmap(0, 0, image_data_FIRSTarray, 128, 64, 1);
      display.display();    // Example action for "sad" emotion
    } else if (incomingEmotion == "angry") {
      display.clearDisplay();  // Clear the buffer
      display.drawBitmap(0, 0, image_data_THIRDarray, 128, 64, 1);
      display.display();    // Example action for "angry" emotion
    } else {
      display.clearDisplay();
      display.setTextSize(2);
      display.setTextColor(WHITE);
      display.setCursor(0, 0);
      display.print("say your feeling");
      display.setCursor(40, 40);
      display.print(millis());
      display.display();   // Default action
    }
  }
